<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>NewLibrary</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>NewLibrary-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>book</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/css/tagify.css</header-portlet-css>
		<header-portlet-javascript>/js/tagify.js</header-portlet-javascript>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
			
		<css-class-wrapper>book-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
